#include "oled.h"
#include <stdint.h>
#include "stm32f446xx_i2c_driver.h"



int main(void)
{
	I2C_Handle_t oled;
	OLED_Init(&oled);
	while(1)
	{
		OLED_Clear(&oled);
			OLED_PrintString(&oled, "anees kokadan");
			printf("Anees Kokadan");
	}




}

