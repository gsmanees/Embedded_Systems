#include <stdint.h>
#include "stm32_f446xx.h"

void delay_ms(uint32_t ms)
{
    volatile uint32_t count;   // volatile prevents optimization

    while(ms--)
    {
        // Adjust this value based on your MCU clock frequency
        for(count = 0; count < 4000; count++)
        {
            // empty loopâ€”no __NOP()
        }
    }

}
int main(void)
{
	GPIOA_CLK_EN(); // Enable GPIO clock
	GPIOA->MODER &= ~(0x3 << (10)); // PA5 set to 00
	GPIOA->MODER|= (1<<10); // PA5 set to 01, which is 01: General purpose output mode
	GPIOA->OTYPER &=~(1<<5); // output type set to push pull state
	GPIOA->OSPEEDR &=~(0x3<<10); // 5th bit set to 0
	GPIOA->OSPEEDR|= (1<<10); // 5th bit set to 01, for medium speed
	GPIOA->PUPDR&= ~(0x3 << (10)); // 5th bit set to 00, means no pull up and  pull down
	GPIOA->ODR&=~(1<<5); // to set PA5 to 0
	//GPIOA->ODR|=(1<<5); // to set PA5 to 1
	//for(;;);


    while(1)
    {
        GPIOA->ODR ^= (1 << 5);   // Toggle LED using XOR
        delay_ms(500);            // 500 ms delay
    }
}

