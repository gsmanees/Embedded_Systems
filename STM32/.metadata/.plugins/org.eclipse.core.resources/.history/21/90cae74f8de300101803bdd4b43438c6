#include <oled.h>
#include <stdint.h>
#include "stm32f446xx_i2c_driver.h"


int main(void)
{
	I2C_Handle_t I2C1Handle;
	OLED_Handle_t OLED;

	/* After I2C init */
	OLED.pI2CHandle = &I2C1Handle;
	OLED.I2C_Address = SSD1306_I2C_ADDR;

	OLED_Init(&OLED);
	OLED_SetCursor(&OLED, 0, 0);
	OLED_WriteString(&OLED, "STM32 OLED");


}

